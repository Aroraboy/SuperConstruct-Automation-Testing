{
  "name": "superconstruct-ai-testing",
  "version": "1.0.0",
  "description": "AI-powered automation testing framework for SuperConstruct",
  "main": "index.js",
  "scripts": {
    "test:gc-full-flow": "playwright test --project=gc-subcontractor-accept --headed",
    "test:gc-register": "playwright test tests/auth/beta-registration-onboarding.spec.js --project=gc-register --headed --no-deps",
    "test:gc-project": "playwright test tests/auth/beta-create-project.spec.js --project=gc-project --headed --no-deps",
    "test:gc-invite": "playwright test tests/auth/gc-invite-members.spec.js --project=gc-invite --headed --no-deps",
    "test:gc-accept": "playwright test tests/auth/accept-invitations-gc.spec.js --project=gc-accept --headed --no-deps",
    "test:gc-add-to-project": "playwright test tests/auth/gc-add-members-to-project.spec.js --project=gc-add-to-project --headed --no-deps",
    "test:gc-members-accept-project": "playwright test tests/auth/members-accept-project.spec.js --project=gc-members-accept-project --headed --no-deps",
    "test:gc-invite-owner": "playwright test tests/auth/gc-invite-owner.spec.js --project=gc-invite-owner --headed --no-deps",
    "test:gc-owner-accept": "playwright test tests/auth/accept-owner-invitation.spec.js --project=gc-owner-accept --headed --no-deps",
    "test:gc-invite-subcontractor": "playwright test tests/auth/gc-invite-subcontractor.spec.js --project=gc-invite-subcontractor --headed --no-deps",
    "test:gc-subcontractor-accept": "playwright test tests/auth/accept-subcontractor-invitation.spec.js --project=gc-subcontractor-accept --headed --no-deps",
    "test:sov-flow": "playwright test --project=sov-approve --headed",
    "test:sov-gc-login": "playwright test tests/modules/gc-login.spec.js --project=sov-gc-login --headed --no-deps",
    "test:sov-create": "playwright test tests/modules/create-sov.spec.js --project=sov-create --headed --no-deps",
    "test:sov-owner-login": "playwright test tests/modules/owner-login.spec.js --project=sov-owner-login --headed --no-deps",
    "test:sov-approve": "playwright test tests/modules/approve-sov.spec.js --project=sov-approve --headed --no-deps",
    "report": "playwright show-report reports/html-report",
    "install:browsers": "playwright install chromium"
  },
  "keywords": [
    "automation",
    "testing",
    "playwright",
    "ai",
    "smoke-tests"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@playwright/test": "^1.40.1",
    "dotenv": "^16.3.1",
    "imap-simple": "^5.1.0",
    "imapflow": "^1.2.9",
    "mailparser": "^3.9.3",
    "nodemailer": "^8.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.5"
  }
}
