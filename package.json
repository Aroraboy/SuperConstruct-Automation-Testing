{
  "name": "superconstruct-ai-testing",
  "version": "1.0.0",
  "description": "AI-powered automation testing framework for SuperConstruct",
  "main": "index.js",
  "scripts": {
    "setup": "node scripts/setup.js",
    "postinstall": "node scripts/setup.js",
    "test:smoke": "playwright test tests/smoke-test.spec.js --headed",
    "test:functional": "playwright test tests/comprehensive-functional.spec.js --headed",
    "test:data-driven": "playwright test tests/data-driven-tests.spec.js --headed",
    "test:all": "playwright test",
    "test:auth": "playwright test tests/auth/",
    "test:registration": "playwright test tests/auth/registration-with-otp.spec.js --headed",
    "test:beta-register": "playwright test tests/auth/beta-registration-onboarding.spec.js --project=auth --headed",
    "test:gc-invite": "playwright test tests/auth/gc-invite-members.spec.js --project=auth --headed",
    "test:gc-flow": "playwright test --project=gc-invite --headed",
    "test:gc-accept": "playwright test tests/auth/accept-invitations-gc.spec.js --project=auth --headed",
    "test:gc-add-to-project": "playwright test tests/auth/gc-add-members-to-project.spec.js --project=auth --headed",
    "test:gc-full-flow": "playwright test --project=gc-add-to-project --headed",
    "test:beta-project": "playwright test tests/auth/beta-create-project.spec.js --project=auth --headed",
    "test:beta-invite": "playwright test tests/auth/contractor-login-with-member-invite.spec.js --project=auth --headed",
    "test:register-gmail": "playwright test tests/auth/register-gmail-account.spec.js --project=auth --headed",
    "test:register-members": "playwright test tests/auth/register-all-members.spec.js --project=auth --headed",
    "test:owner-invite": "playwright test tests/auth/owner-invite-members.spec.js --project=auth --headed",
    "test:accept-invitations": "playwright test tests/auth/accept-invitations.spec.js --project=auth --headed",
    "test:full-flow": "playwright test tests/auth/register-all-members.spec.js tests/auth/owner-invite-members.spec.js tests/auth/accept-invitations.spec.js --project=auth --headed --workers=1",
    "test:users": "playwright test tests/modules/users.spec.js",
    "test:modules": "playwright test tests/modules/",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "report": "playwright show-report reports/html-report",
    "dashboard": "node dashboard/server.js",
    "install:browsers": "playwright install chromium"
  },
  "keywords": [
    "automation",
    "testing",
    "playwright",
    "ai",
    "smoke-tests"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@playwright/test": "^1.40.1",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "imap-simple": "^5.1.0",
    "imapflow": "^1.2.9",
    "mailparser": "^3.9.3",
    "mailslurp-client": "^15.16.0",
    "nodemailer": "^8.0.1",
    "openai": "^4.20.1",
    "tesseract.js": "^7.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.5"
  }
}
